{
    "name": "YouTube to MP3",
    "description": "Youtube'da şarkı dinlerken tek tık ile indirin.",
    "version": "1.1",
    "manifest_version": 2,
    "author": "YK Yazılım",
    "browser_action": {
        "default_icon": {
            "128": "img/youtube.png"
        },
        "default_title": "YouTube to MP3"
    },
    "content_scripts": [{
        "matches": ["*://*.youtube.com/*"],
        "js": [
            "js/jquery.min.js",
            "js/main.js"
        ]
    }]
}
